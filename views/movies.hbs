   <h1>Ironhack Cinema</h1>
    <h2>Click on the movie to see the showtimes!</h2>
<div class="movieList">
  {{#each movies}}
    <div class="movie">
      <img src="{{this.image}}" alt="Movie Poster" height="500px" />
      <h2>{{this.title}}</h2>
      <a class="movie-link" href="/movies/{{this.id}}">See more</a>
    </div>
  {{/each}}
</div>